@model CRUDangular_1.ViewModel.LayoutModel
<html style="height:auto">
<head>
    <link rel="shortcut icon" href="~/Images/Icon/logo.svg"></link>
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/bundles/mini-spa/style")
    @Scripts.Render("~/bundles/mini-spa/script")
    <nav class="navbar" >
      <nav class="navbar navbar-inverse navbar-static-top extended-nav nac-main-container global-alert-offset-top" style="background-color:white">
        <div class="container">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar3">
              <span class="sr-only"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a id="starry" class="navbar-brand navbar-text"><img src="../../Images/Icon/Picture1.png" style="margin-top:-7%" alt="Aiou Rwp Region"/></a>
            </div>
          <div id="navbar3" class="navbar-collapse collapse">
            @using (Html.BeginForm("LogOff", "Home", FormMethod.Post, new { id = "logoutForm", @class = "nav" }))
              {
                <ul class="nav navbar-nav navbar-right">
                <li>@Html.ActionLink("Home", "Index", "Home", routeValues: null, htmlAttributes: new { title = "Manage" })</li>
               <li>
                 <a href="javascript:document.getElementById('logoutForm').submit()">Log off</a>
               </li>
            
             </ul> 
            }
          </div>
          <!--/.nav-collapse -->
        </div>
        <!--/.container-fluid -->
      </nav>
    </nav>
</head>
<body class="body" style="height:auto;width:auto; background-color:#e1e9ee">
    <div class="container" >
        @RenderBody()
    </div>
</body>
</html>